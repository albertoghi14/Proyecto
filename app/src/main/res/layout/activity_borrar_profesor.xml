<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BorrarProfesor">

    <Button
        android:id="@+id/buttonCancelarBorrarProfesor"
        android:layout_width="194dp"
        android:layout_height="62dp"
        android:layout_marginTop="4dp"
        android:text="@string/button_cancelar_borrar_profesor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonBorrarProfesor" />

    <Button
        android:id="@+id/buttonBorrarProfesor"
        android:layout_width="194dp"
        android:layout_height="62dp"
        android:layout_marginTop="84dp"
        android:text="@string/button_borrar_profesor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerBorrarProfesor" />

    <Spinner
        android:id="@+id/spinnerBorrarProfesor"
        android:layout_width="311dp"
        android:layout_height="41dp"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTituloBorrarProfesor" />

    <TextView
        android:id="@+id/textViewTituloBorrarProfesor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViex_titulo_borrar_profesor"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.054" />
</androidx.constraintlayout.widget.ConstraintLayout>